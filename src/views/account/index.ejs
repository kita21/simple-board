<!DOCTYPE html>
<html>
    <body>
        <h1>管理者画面</h1>
        <p><a href="/account/create">ユーザ作成</a></p>
        <p><%= msg %></p>
        <table>
            <% userList.forEach(function (user) { %>
            <tr>
                <th><%= user.dataValues.id %></th>
                <td><%= user.dataValues.name %></td>
                <td><a href="/account/edit/<%= user.dataValues.id %>">変更</a> </td>
                <td>
                    <form method="post" action="/account/delete">
                        <input type="hidden" name="id" value="<%= user.dataValues.id %>">
                        <input type=submit value="削除">
                    </form>
                </td>
            </tr>
            <% }); %>
        </table>
    </body>
</html>